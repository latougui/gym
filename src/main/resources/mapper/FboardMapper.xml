<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.latou.gym.mapper.FboardMapper">

    <select id="findAllFboard"  resultType="Fboard">
        select * from fboard
    </select>

    <select id="findByBoardId"  resultType="Fboard">
        select * from fboard where fboard_id=#{fboardId}
    </select>
</mapper>